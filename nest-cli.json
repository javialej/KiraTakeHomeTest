{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true
  },
  "schematics": {
    "vendor": {
      "factory": "./schematics/vendor/index#vendor",
      "schema": "./schematics/vendor/schema.json"
    }
  }
}
